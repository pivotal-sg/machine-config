[core]
  excludesfile = ~/.gitignore_global

[user]
  name = Pair
  email = pair@pivotal.io
[push]
  default = simple

[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff --cached
  lg = log -p
  who = shortlog -s --
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  dc = duet-commit

[init]
  templatedir = ~/.git-template-with-git-hooks
